{
  "name": "skillhub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "cleanwin": "if exist .next rd /s /q .next && if exist node_modules\\.cache rd /s /q node_modules\\.cache",
    "dev:clean": "npm run cleanwin && next dev",
    "make-admin": "node -e \"import('./scripts/make-admin-commonjs.js').then(m => m.default())\""
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "next": "15.3.0",
    "nodemailer": "^6.10.1",
    "react": "^19.0.0",
    "react-big-calendar": "^1.18.0",
    "react-dom": "^19.0.0",
    "resend": "^4.4.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.3"
  }
}
